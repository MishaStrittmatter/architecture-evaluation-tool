<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>

   <extension
         point="org.eclipse.ui.views">
      <category
            name="Code and Model Analysis"
            id="de.cau.cs.se.software.evaluation">
      </category>
      <view
            name="Analysis Result View"
            icon="icons/sample.gif"
            category="de.cau.cs.se.software.evaluation"
            class="de.cau.cs.se.software.evaluation.views.AnalysisResultView"
            id="de.cau.cs.se.software.evaluation.views.AnalysisResultView">
      </view>
      <view
            category="de.cau.cs.se.software.evaluation"
            class="de.cau.cs.se.software.evaluation.views.LogView"
            icon="icons/sample.gif"
            id="de.cau.cs.se.software.evaluation.views.LogView"
            name="Analysis Log View">
      </view>
   </extension>
   <extension
         point="org.eclipse.ui.perspectiveExtensions">
      <perspectiveExtension
            targetID="org.eclipse.jdt.ui.JavaPerspective">
         <view
               ratio="0.5"
               relative="org.eclipse.ui.views.ProblemView"
               relationship="right"
               id="de.cau.cs.se.software.evaluation.views.AnalysisResultView">
         </view>
      </perspectiveExtension>
   </extension>
   <extension point="org.eclipse.emf.ecore.generated_package">
      <!-- @generated HypergraphMetricsState -->
      <package
            uri="http://evalutation.se.cs.cau.de/hypergraph/metrics/state"
            class="de.cau.cs.se.software.evaluation.state.StatePackage"
            genModel="model/HypergraphState.genmodel"/>
   </extension>
   <extension
         point="org.eclipse.ui.commands">
      <command
            defaultHandler="de.cau.cs.se.software.evaluation.commands.JavaProjectAnalysisHandler"
            id="de.cau.cs.se.software.evaluation.commands.analysis.java"
            name="Analysis Java">
      </command>
      <command
            defaultHandler="de.cau.cs.se.software.evaluation.commands.ModelAnalysisHandler"
            id="de.cau.cs.se.software.evaluation.commands.analysis.model"
            name="Analysis Model">
      </command>
   </extension>
   <extension
         point="org.eclipse.ui.menus">
      <menuContribution
        locationURI="popup:org.eclipse.jdt.ui.PackageExplorer?after=additions">
	     <!-- menu
	           id="de.cau.cs.se.software.evaluation.popups"
	           label="Analysis Complexity">
	     </menu -->
      <command
            commandId="de.cau.cs.se.software.evaluation.commands.analysis.java"
            label="Analysis Java"
            style="push"
            tooltip="Run Java analysis">
      </command>
      <command
            commandId="de.cau.cs.se.software.evaluation.commands.analysis.model"
            label="Analysis Model"
            style="push"
            tooltip="Run model analysis">
      </command>
	  </menuContribution>
	  <menuContribution
        locationURI="popup:org.eclipse.ui.navigator.ProjectExplorer#PopupMenu?after=additions">
	     <!-- menu
	           id="de.cau.cs.se.software.evaluation.popups"
	           label="Analysis Complexity">
	     </menu -->
	     <command
	           commandId="de.cau.cs.se.software.evaluation.commands.analysis.java"
	                  label="Analysis Java"
	                  style="push"
	                  tooltip="Run Java analysis">
	     </command>
      <command
            commandId="de.cau.cs.se.software.evaluation.commands.analysis.model"
            label="Analysis Model"
            style="push"
            tooltip="Run model analysis">
      </command>
	  </menuContribution>
	  <menuContribution
	        locationURI="popup:de.cau.cs.se.software.evaluation.popups?after=additions">
	     <command
	           commandId="de.cau.cs.se.software.evaluation.commands.analysis.java"
	                  label="Analysis Java"
	                  style="push"
	                  tooltip="Run Java analysis">
	     </command>
      <command
            commandId="de.cau.cs.se.software.evaluation.commands.analysis.model"
            label="Analysis Model"
            style="push"
            tooltip="Run model analysis">
      </command>
      </menuContribution>
   </extension>

   <extension point="org.eclipse.emf.ecore.generated_package">
      <!-- @generated Hypergraph -->
      <package
            uri="http://evaluation.se.cs.cau.de/hypergraph"
            class="de.cau.cs.se.software.evaluation.hypergraph.HypergraphPackage"
            genModel="model/Hypergraph.genmodel"/>
   </extension>

   <extension point="org.eclipse.emf.ecore.generated_package">
      <!-- @generated HypergraphState -->
      <package
            uri="http://evalutation.se.cs.cau.de/hypergraph/metrics/state"
            class="de.cau.cs.se.software.evaluation.state.StatePackage"
            genModel="model/HypergraphState.genmodel"/>
   </extension>

</plugin>
